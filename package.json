{
  "private": true,
  "workspaces": {
    "packages": [
      "api",
      "web"
    ]
  },
  "devDependencies": {
    "@redwoodjs/core": "8.0.0-canary.417",
    "@redwoodjs/project-config": "8.0.0-canary.417"
  },
  "eslintConfig": {
    "extends": "@redwoodjs/eslint-config",
    "root": true
  },
  "engines": {
    "node": "=20.x"
  },
  "prisma": {
    "seed": "yarn rw exec seed"
  },
  "packageManager": "yarn@4.1.1",
  "scripts": {
    "bs": "yarn rw build && yarn sed && yarn copy && yarn rw serve",
    "copy": "cp -R web/src/docs web/dist/rsc/docs",
    "sed": "sed -i '' 's/docs\\/(.*)/docs\\/*/g' web/dist/server/route-manifest.json"
  }
}
